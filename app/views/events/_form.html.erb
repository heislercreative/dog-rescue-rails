<%= render 'errors' %>

<%= form_for @event do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>

  <%= f.label :date %>
  <%= f.date_field :date %><br>

  <%= f.label :description %><br>
  <%= f.text_area :description %><br><br>

  <%= f.hidden_field :organizer_id %>

  <%= f.submit class: "btn btn-warning" %>

<% end %>
