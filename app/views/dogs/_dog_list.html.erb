<tbody>
  <% @dogs.each do |dog| %>
    <td class="list-item">
      <tr>
        <div class="info col col-sm-4">
          <hr>
          <h4><%= link_to dog.name, dog %></h4>
          <p><em><%= link_to dog.breed.name, dog.breed %></em></p>
          <p><%= dog.gender %> - <%= dog.age %> years old</p>
          <p>Potty Trained: <%= is_trained?(dog) %></p>
        </div>
      </tr>
    </td>
  <% end %>
</tbody>
